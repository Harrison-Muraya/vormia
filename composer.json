{
    "name": "vormia-cms/starterkit",
    "version": "0.1.3", 
    "description": "Vormia CMS Starter Kit for Laravel 12",
    "type": "library",
    "homepage": "https://github.com/vormia-cms/starterkit",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Minga",
            "email": "joshlminga@gmail.com",
            "homepage": "https://vormia.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "VormiaCms\\StarterKit\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VormiaCms\\StarterKit\\StarterServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "intervention/image": "^3.6",
        "illuminate/filesystem": "^12.0",
        "livewire/livewire": "^3.5",
        "laravel/sanctum": "^4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}